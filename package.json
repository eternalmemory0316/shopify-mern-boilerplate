    "express-session": "^1.15.6",
    "express-status-monitor": "^1.0.1",
    "express-validator": "^5.2.0",
    "lodash-decorators": "^5.0.0",
    "lusca": "^1.6.0",
    "mongoose": "^5.0.17",
    "morgan": "^1.9.0",
    "multer": "^2.0.0-alpha.6",
    "nodemailer": "^4.6.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
