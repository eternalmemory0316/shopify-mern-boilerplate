{
  "name": "my-new-project",
  "version": "5.0.0",
  "description": "",
  "author": "Minh Phan",
  "license": "ISC",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon ./index.js --exec babel-node --presets @babel/env --ignore client",
    "test": "nyc mocha  --compilers js:@babel/register --timeout=3000 --exit",
    "lint": "eslint server/**/*.js client/**/*.js  --ignore-pattern public",
    "build": "yarn clean && yarn build:server && yarn build:client",
    "serve": "yarn build && NODE_ENV=production node server.js",
    "deploy": "yarn build && NODE_ENV=production pm2 start server.js -i max --name boost-solution -o logs/pm2.log --merge-logs -e logs/pm2-e.log",
    "restart": "yarn build && NODE_ENV=production pm2 restart server.js -i max --name boost-solution -o logs/pm2.log --merge-logs -e logs/pm2-e.log",
    "stop": "pm2 stop boost-solution",
    "delete": "pm2 delete boost-solution",
    "build:server": "cross-env BABEL_ENV=node NODE_ENV=production babel server -d dist --ignore public && cross-env BABEL_ENV=node NODE_ENV=production babel index.js -o server.js",
    "build:client": "cross-env BABEL_ENV=react-production NODE_ENV=production webpack --config ./client/webpack.config.js --progress --profile --colors",
    "clean": "rm -rf ./assets && mkdir ./assets && rm -rf ./dist && mkdir ./dist"
  },
  "dependencies": {
    "@shopify/polaris": "^2.0.0",
    "@shopify/shopify-express": "https://github.com/paul-phan/shopify-express",
    "bcrypt-nodejs": "^0.0.3",
